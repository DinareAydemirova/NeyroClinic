@model Labaratory
<div class="card-body">
    <h5 class="card-title">AEdit Labaratory Results</h5>

    <!-- Multi Columns Form -->
    <form class="row g-3" method="post">
        <div class="col-md-6">
            <div class="mb-3">
                <label class="form-label" asp-for="Patient"></label>
                <input asp-for="Patient" class="form-control">
                <span class="text-danger" asp-validation-for="Patient"></span>
            </div>

        </div>
        <div class="col-md-6">
            <div class="mb-3">
                <label class="form-label" asp-for="Category"></label>
                <input asp-for="Category" class="form-control">
                <span class="text-danger" asp-validation-for="Category"></span>
            </div>
        </div>

        <div class="col-6">
            <div class="mb-3">
                <label class="form-label" asp-for="Resault"></label>
                <input asp-for="Resault" class="form-control">
                <span class="text-danger" asp-validation-for="Resault"></span>
            </div>
        </div>
        <div class="col-md-6">
            <div class="mb-3">
                <label class="form-label" asp-for="Date"></label>
                <input asp-for="Date" class="form-control">
                <span class="text-danger" asp-validation-for="Date"></span>
            </div>
        </div>





        <div class="text-center">

            <button type="submit" class="btn btn-primary">Submit</button>
            <a asp-action="Index" class="btn btn-secondary text-white">Go back</a>



        </div>
    </form><!-- End Multi Columns Form -->

</div>

